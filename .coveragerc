[run]
branch = True
include =
    ./src/*
    ./scrape_web.py
omit =
    ./.idea/*
    ./.pytest_cache/*
    ./.vscode/*
    ./alembic/*
    ./files/*
    ./htmlcov/*
    ./junit/*
    ./sample_data/*
    ./scripts/*
    ./tests/*
    ./utility-repo-scripts/*
    */__pycache__/*
    ./setup.py
concurrency = thread,greenlet
[report]
precision = 2
show_missing = True
skip_empty = True
sort = Name
[html]
directory = ./htmlcov
skip_empty = True
title = python-genshin-impact-web-scraping Coverage Report
[xml]
output = ./junit/coverage.xml
